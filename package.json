{
  "name": "@balena/jellyfish-chat-widget",
  "version": "6.0.66",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-chat-widget.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Chat widget library for Jellyfish",
  "main": "lib/index.jsx",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js,jsx}"
    ],
    "modules": [
      "lib"
    ]
  },
  "ava": {
    "timeout": "2m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": true,
    "extensions": [
      "jsx"
    ],
    "babel": {
      "testOptions": {
        "plugins": [
          "@babel/plugin-syntax-jsx"
        ]
      }
    },
    "files": [
      "lib/**/*.spec.{js,jsx}"
    ],
    "require": [
      "./test/browser-setup",
      "./test/register",
      "./test/max-listeners"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --fix",
      "npm run doc",
      "git add README.md"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/*.jsx lib/**/*.jsx lib/**/*.js > README.md"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-ui-components": "^8.9.0",
    "catch-uncommitted": "^1.6.2",
    "immutability-helper": "^3.1.1",
    "jsdoc-to-markdown": "^7.0.0",
    "lodash": "^4.17.21",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rendition": "^20.10.1",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2",
    "xterm": "4.9.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "@balena/jellycheck": "^0.1.1",
    "ava": "^3.15.0",
    "bluebird": "^3.7.2",
    "browser-env": "^3.3.0",
    "depcheck": "^1.4.0",
    "deplint": "^1.1.3",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "lint-staged": "^10.5.4",
    "node-hook": "^1.0.0",
    "simple-git-hooks": "^2.2.0",
    "sinon": "^9.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
